<?xml version="1.0" encoding="UTF-8"?>
<pages:BasePage xmlns:pages="clr-namespace:MyWasteDriver.UI.Pages"
			 xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyWasteDriver.UI.Pages.Work.OrderInfoPage">

	<ContentPage.Content>
		<StackLayout>

			<Label Text="Order info page"
		  VerticalOptions="CenterAndExpand"
	      HorizontalOptions="CenterAndExpand"/>
			<Button Text="GoToCurrentOrder"
				  Command="{Binding GoToCurrentOrderCommand}"
				  VerticalOptions="CenterAndExpand"
				  HorizontalOptions="CenterAndExpand"/>






		</StackLayout>
	</ContentPage.Content>
</pages:BasePage>